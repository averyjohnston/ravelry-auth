{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es2022",
    "lib": ["es2022"],
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "module": "NodeNext",
    "outDir": "dist",
  }
}
